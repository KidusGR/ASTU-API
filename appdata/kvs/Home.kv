<HomeScreen>:
    name: "Home"
    id: home
    FitImage:
        source:"appdata/image/astu_green.png"
        size_hint: 1, None
        size: "30dp", root.width
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        opacity: 0.15
    MDNavigationLayout:
        ScreenManager:
            Screen:
                BoxLayout:
                    orientation: "vertical"
                    size_hint: 1, 1
                    md_bg_color: (30/255,30/255,30/255,1)
                    MDTopAppBar:
                        title: "[font=appdata/font/neuropol.otf][size=14]Home[/font][/size]"
                        md_bg_color: app.theme_cls.primary_light
                        elevation: 0
                        specific_text_color: (30/255,30/255,30/255,1)
                        left_action_items: [["menu", lambda x: nav_drawer.set_state('toggle')]]
                        right_action_items: [["reload"]]
                        
                    MDFloatLayout:
                        id: floatH
                        FitImage:
                            id: profile
                            pos_hint: {"center_x": 0.5,"center_y": 0.78}
                            size_hint: None, None
                            size: "200dp", "200dp"
                            radius: 0
                        MDLabel:
                            id: labelH
                            pos_hint: {"center_x": 0.5, "center_y": 0.55}
                            halign: "center"
                            theme_text_color: "Custom"
                            text_color: "white"
                            font_name: 'appdata/font/neuropol.otf'
                            font_size: "15dp"
                        MDBoxLayout:
                            id: boxH
                            md_bg_color: (30/255, 30/255, 30/255, 1)
                            size_hint: 1, None
                            height: "240dp"
                            pos_hint: {"center_x": 0.5, "center_y": 0.30}
                            orientation: "vertical"
                            
                            OneLineIconListItem:
                                bg_color: (1,1,1,0.2)
                                pos_hint: {"center_x": 0.5, "center_y": 1}
                                size_hint: 1, None
                                height: "25dp"
                                MDIcon:
                                    icon: "home"
                                    pos_hint: {"center_x": 0.05, "center_y": 0.6}
                                    padding_x: "15dp"
                                    font_size: "15dp"
                                    theme_text_color: "Custom"
                                    text_color: self.theme_cls.primary_color
                                    halign: "center"
                                MDLabel:
                                    text: "Student Information"
                                    text_align: "center"
                                    theme_text_color: "Custom"
                                    text_color: self.theme_cls.primary_color
                                    pos_hint: {"center_x": 0.5, "center_y": 0.5}
                                    font_name: 'appdata/font/neuropol.otf'
                                    theme_text_color: "Custom"
                                    text_color: app.theme_cls.primary_color
                                    font_size: "10dp"
                                    padding_x: "25dp"
                                    





