        MDNavigationDrawer:
            id: nav_drawer
            radius: 0

            MDBoxLayout:
                size_hint: 1, 1
                orientation: "vertical"
                spacing: "5dp"

                FitImage:
                    size_hint: None, None
                    source: "appdata/image/NoUser.jpg"
                    radius: 360
                    pos_hint: {"center_x": 0.5}
                Widget:
                    size_hint_y: None
                    height: "10dp"
                MDLabel:
                    md_bg_color: app.theme_cls.primary_color
                    text: 'Name : "Student"'
